<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.ComboBox?>
<?import java.lang.String?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="CreateOrder"
      stylesheets="stylesheet.css"
      prefHeight="400.0" prefWidth="600.0">

    <VBox id="orderBox">

        <ComboBox value="Select Customer">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Test"/>
                </FXCollections>
            </items>
        </ComboBox>

        <ComboBox value="Select Operator">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Test"/>
                </FXCollections>
            </items>
        </ComboBox>



    </VBox>



    <VBox id="buttonAlignment">
        <Button id="placeOrderButton" text="Place Order" onMouseClicked="#placeOrderButtonClicked"/>
        <Button id="goBackButton" text="Go Back" onMouseClicked="#goBackButtonClicked"/>
    </VBox>


</VBox>
